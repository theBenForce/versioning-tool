{
  "name": "versioning-tool",
  "description": "CLI tool for updating npm package version and tagging git repos for release.",
  "version": "2018.10.08-4",
  "author": "Ben Force",
  "versioning": {
    "date-format": "yyyy.mm.d"
  },
  "bin": {
    "vt": "./bin/run"
  },
  "bugs": "https://github.com/theBenForce/versioning-tool/issues",
  "dependencies": {
    "@oclif/command": "^1.5.2",
    "@oclif/config": "^1.8.4",
    "@oclif/plugin-help": "^2.1.2",
    "@types/dateformat": "^1.0.1",
    "@types/shelljs": "^0.8.0",
    "cli-ux": "^4.8.2",
    "dateformat": "^3.0.3",
    "shelljs": "^0.8.2",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.4",
    "globby": "^8.0.1",
    "ts-node": "^6.2.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/theBenForce/versioning-tool",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "vt",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "theBenForce/versioning-tool",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p . --noEmit && tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
